<%= flash[:notice] %>
<table>
  <thead>
  <tr>
    <th>id</th>
    <th>fname</th>
    <th>lname</th>
    <th>race</th>
    <th>gender</th>
    <th>ssn</th>
    <th>dob</th>
  </tr>
  </thead>
  <tbody>
  <% @dbases.each do |db| %>
      <tr>
        <td><%= db.id %></td>
        <td><%= db.fname %></td>
        <td><%= db.lname %></td>
        <td><%= db.race %></td>
        <td><%= db.gender %></td>
        <td><%= db.ssn %></td>
        <td><%= db.dob %></td>
      </tr>
  <% end %>
  </tbody>
</table>
<div>
  <h3>Upload a CSV File</h3>
  <%= form_tag upload_dbases_path, multipart: true do %>
      <%= file_field_tag :file %>
      <%= submit_tag 'Upload CSV' %>
  <% end %>
</div>
</div>
